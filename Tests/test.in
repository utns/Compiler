type
    point = record
        x, y: integer;
    end;

procedure f(var p: point);
begin
    writeln(p.x, ' ', p.y);
end;

procedure g();
var
    p: point;
begin
    p.x := 3;
    p.y := 2;
    f(p);
    writeln(p.x, ' ', p.y);
end;

var
    p: point;
begin
    p.x := 1;
    p.y := 2;
    f(p);
    g();
end.

